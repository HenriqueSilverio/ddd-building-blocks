{
  "name": "ddd-building-blocks",
  "version": "1.0.0",
  "description": "Domain-Driven Design tactical modeling components",
  "engines": {
    "node": "^16.14.2",
    "npm": "^8.5.0"
  },
  "main": "src/index.ts",
  "scripts": {
